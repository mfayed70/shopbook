<?xml version="1.0" encoding="UTF-8"?>
<logging_configuration>
    <loggers>
        <logger name="oracle.adf.model.log.AMPoolLifecycle"/>
        <logger name="oracle.adf.model.log.LobDomain"/>
        <logger name="oracle.adf.model.log.SQLBuilder"/>
        <logger name="oracle.adf.model.log.DataChangeEvent"/>
        <logger name="oracle.adf.model.log.JPXSettings"/>
        <logger name="oracle.adf.model.log.GlobalGroovyFunctions"/>
        <logger name="oracle.adf.model.log.ApplicationScript" />
        <logger name="oracle.adf.model.log.FrameManagement"/>
        <logger name="oracle.adf.model.log.BusEvent"/>
        <logger name="oracle.adf.model.log.DefaultValueSerializer"/>
        <!--
            BEGIN SECTION: ADFm Business Editor Functional Loggers
         -->
        <logger name="oracle.adf.model.businesseditor.log"/>
        <!-- configuration -->
        <logger name="oracle.adf.model.businesseditor.log.Settings"/>
        <!-- Allocation/Management/Migration of objects in the
