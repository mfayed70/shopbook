        <logger name="oracle.adf.model.businesseditor.log.customization.logic.Functions"/>
        <logger name="oracle.adf.model.businesseditor.log.customization.logic.Triggers"/>
        <logger name="oracle.adf.model.businesseditor.log.customization.logic.ValidationRules"/>
        <logger name="oracle.adf.model.businesseditor.log.customization.WebServices"/>
        <!--
            END SECTION: ADFm Business Editor Functional Loggers
         -->
        <logger name="oracle.adfdebugdiagnostics"/>
    </loggers>
</logging_configuration>
