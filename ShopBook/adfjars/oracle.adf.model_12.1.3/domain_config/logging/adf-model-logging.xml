        <logger name="oracle.adf.model.businesseditor.log.dictionary"/>
        <logger name="oracle.adf.model.businesseditor.log.dictionary.Allocation"/>
        <logger name="oracle.adf.model.businesseditor.log.dictionary.Lookups"/>
