  <SQLQuery><![CDATA[SELECT 
    CatMnfBrndMdlYers.CMBMY_CODE, 
    CatMnfBrndMdlYers.CAT_CODE, 
    CatMnfBrndMdlYers.MNF_CODE, 
    CatMnfBrndMdlYers.BRAND_CODE, 
    CatMnfBrndMdlYers.MODEL_CODE, 
    if (CatMnfBrndMdlYers.YEAR=0,'',CatMnfBrndMdlYers.YEAR) as YEAR,
    CatMnfBrndMdlYers.LAST_MODIFIED, 
    CatMnfBrndMdlYers.NOTES, 
    brands.BRAND_NAME BRAND_NAME, 
    manufactures.MNF_NAME MNF_NAME, 
    models.MODEL_NAME MODEL_NAME, 
    ssc.SHOP_SUB_CAT_CODE
FROM 
    cat_mnf_brnd_mdl_yers CatMnfBrndMdlYers 
    join brands 
    on CatMnfBrndMdlYers.BRAND_CODE = brands.BRAND_CODE
    join manufactures 
    on CatMnfBrndMdlYers.MNF_CODE = manufactures.MNF_CODE
    join models
    on CatMnfBrndMdlYers.MODEL_CODE = models.MODEL_CODE
	,shop_sub_cats ssc, shop_categories sc
WHERE 
    CatMnfBrndMdlYers.CAT_CODE = sc.CAT_CODE and
    ssc.SHOP_CAT_CODE = sc.SHOP_CAT_CODE]]></SQLQuery>
